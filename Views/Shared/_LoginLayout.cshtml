<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title - DiziSearch</title>
    <link href="https://fonts.googleapis.com/css?family=Teko" rel="stylesheet">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css" integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
</head>
<body class="body">
    <div class="body-out-login" style="">
        <div class="container body-content-login" style="margin:60px auto -236px;min-height:100vh;background-color:transparent;max-width:450px;color:#333;padding:0 5%;">
            <main role="main" class="">
                @RenderBody()
            </main>
        </div>
    </div>

    <footer style="border-top:2px ridge #808080;background-color:rgba(0,0,0,.8);" class="footer fixed-bottom text-muted">
        <div class="container" style="color:#ececeb;width:100%;height:100%;">
            &copy; 2019 - Designed by website owner
        </div>
    </footer>

    <script src="~/js/site.js"></script>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
